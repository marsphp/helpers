{
    "name": "mars/helpers",
    "description": "Mars PHP framework helpers package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Houssene Dao",
            "email": "dao.houssene@themartiangeeks.com"
        },
        {
            "name": "Hassane Dao",
            "email": "dao.hassane@themartiangeeks.com"
        }
    ],
    "support": {
        "issues": "https://github.com/marsphp/helpers/issues",
        "source": "https://github.com/marsphp/helpers"
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Mars\\Helpers\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
